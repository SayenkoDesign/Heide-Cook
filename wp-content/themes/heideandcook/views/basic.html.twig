        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
        <title> {{ title }}</title>
    {% endblock %}

    {% block stylesheets %}
        {{ head|raw }}
    {% endblock %}

</head>
<body>
{% block body %}
    {% block header %}
        {% include "layout/header.html.twig" %}
    {% endblock %}

    {% block content %}
        <section class="content">
            <div class="row column">
                <h1>Test</h1>
            </div>
        </section>
    {% endblock %}

    {% block footer %}
        {% include "layout/footer.html.twig" %}
    {% endblock %}
{% endblock %}

{% block scripts %}

{% endblock %}
</body>
</html>
