<html>
<head>
    {% block meta %}
        <meta charset="utf-8">
        <meta name="viewport" content="height=device-height,width=device-width,initial-scale=1">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
        <title>{{ SP.WordPress.title() }}</title>
    {% endblock %}

    {% block stylesheets %}
        {{ SP.WordPress.header() }}
    {% endblock %}
</head>
<body {{ SP.WordPress.classes() }}>
{% block menu %}
    {% include "layout/menu.html.twig" %}
{% endblock %}
{% block body %}
    {% block header %}
        {% include "layout/header.html.twig" %}
    {% endblock %}

    {% block content %}
        {% if content is defined and content|length %}
            {% for row in content %}
                {{ row }}
            {% endfor %}
        {% endif %}
    {% endblock %}

    {% block locations %}
        {% include "layout/locations.html.twig" %}
    {% endblock %}
    {% block footer %}
        {% include "layout/footer.html.twig" %}
    {% endblock %}
{% endblock %}

{% block scripts %}
    {{ SP.WordPress.footer() }}
{% endblock %}
</body>
</html>
